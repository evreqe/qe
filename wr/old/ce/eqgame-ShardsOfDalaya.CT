maphack_PI:
dd (float)3.141592741

maphack_90f:
dd (float)90.0

maphack_180f:
dd (float)180.0

maphack_360f:
dd (float)360.0

maphack_128f:
dd (float)128.0

maphack_256f:
dd (float)256.0

maphack_512f:
dd (float)512.0

maphack_arrow_distance:
dd (float)40.0

maphack_arrow_text:
db 2B 00
// +

maphack_height_filter:
db 00

maphack_zoom:
dd (float)1.0

maphack_draw_line_to_target:
db 01

maphack_font_style:
db 03
// default value is 02, valid values are 01-09
// value of 03 is less blurry and easy to read
// value of 06 or 08 is huge bold text
// value is 09 is the smallest text

maphack_spawn_name_string:
db 2B 20 25 73 00
// + %s

maphack_player_name:
db 58 00
// X

maphack:

code:
  // original code
  call eqgame.exe+19B2D9
  add ebp,68

  // start of our code

  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  cmp dword ptr [ecx+000005B4],05 // check if game state equals in game
  jne code2

  mov ecx,[eqgame.exe+2152C0] // camera
  mov eax,[ecx+00000048] // resolution width
  sar eax,01 // divide by 2
  mov [maphack_origin_x],eax

  mov ecx,[eqgame.exe+2152C0] // camera
  mov eax,[ecx+0000004C] // resolution height
  sar eax,01 // divide by 2
  mov [maphack_origin_y],eax

  mov ecx,[eqgame.exe+32B7A0] // player spawn
  test ecx,ecx // check if spawn is NULL
  jz code2

  cmp byte ptr [ecx+000000E6],01 // is linkdead
  je code2

  fld [ecx+00000068] // y
  fmul [maphack_zoom]
  fchs // reverse sign
  fstp [maphack_player_yf]
  fld [ecx+0000006C] // x
  fmul [maphack_zoom]
  fchs
  fstp [maphack_player_xf]

  fld [ecx+00000068] // y
  fmul [maphack_zoom]
  fchs // reverse sign
  fistp [maphack_player_y] // store float as int
  fld [ecx+0000006C] // x
  fmul [maphack_zoom]
  fchs
  fistp [maphack_player_x]
  fld [ecx+00000070] // z
  fistp [maphack_player_z]

  fld [ecx+00000084] // heading
  fstp [maphack_player_heading]

  mov ecx,[eqgame.exe+32B768] // spawn begin
  test ecx,ecx // check if spawn is NULL
  jz code2

  mov [maphack_spawn_holder],ecx

  // set the font size / style
  mov eax,[maphack_font_style]
  test eax,eax
  jz code2

  mov ecx,[eqgame.exe+39CD38] // ptr to font info from CDisplay::WriteTextHD2
  test ecx,ecx
  jz code2
  mov ecx,[ecx+000000D4]
  test ecx,ecx
  jz code2
  mov ecx,[ecx+00000008]
  test ecx,ecx
  jz code2
  mov [ecx+00000004],eax // offset to font size / style

  jmp spawn_loop

spawn_loop:

  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  cmp dword ptr [ecx+000005B4],05 // check if game state equals in game
  jne code2

  mov ecx,[maphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  fld [ecx+00000068] // y
  fmul [maphack_zoom]
  fchs
  fistp [maphack_spawn_y]
  fld [ecx+0000006C] // x
  fmul [maphack_zoom]
  fchs
  fistp [maphack_spawn_x]
  fld [ecx+00000070] // z
  fistp [maphack_spawn_z]

  mov eax,[maphack_height_filter]
  test eax,eax
  jnz heightfilter_begin
  jmp heightfilter_end

heightfilter_begin:

  // calculate height difference
  mov eax,[maphack_player_z]
  sub eax,[maphack_spawn_z]

  // absolute value
  cdq
  xor eax,edx
  sub eax,edx

  cmp eax,[maphack_height_filter]
  jg spawn_next

  jmp heightfilter_end

heightfilter_end:

  lea eax,[ecx+00000001] // offset to name
  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  push eax
  call eqgame.exe+9087C // trim name to have no zeros or underscores
  push eax
  lea eax,[maphack_spawn_name_display] // string that holds the result of sprintf
  push maphack_spawn_name_string // + %s
  push eax
  call eqgame.exe+19B58B // _sprintf

  mov ecx,[maphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  jmp appendtoname_begin

appendtoname_begin:

  cmp byte ptr [ecx+0000016C],00 // type is player
  jne appendtoname_end

  cmp byte ptr [ecx+00000194],01 // is afk
  je appendtoname_afk_begin
  jmp appendtoname_afk_end

appendtoname_afk_begin:
  mov ecx,[eqgame.exe+32B758] // ptr
  push 00
  push 00003017 // _AFK
  call eqgame.exe+B48A4 // get string by id from eqstr_xx.txt

  // append to name
  push eax
  lea eax,[maphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_afk_end

appendtoname_afk_end:
  mov ecx,[maphack_spawn_holder]
  cmp byte ptr [ecx+000000E6],01 // is linkdead
  je appendtoname_ld_begin
  jmp appendtoname_ld_end

appendtoname_ld_begin:
  mov ecx,[eqgame.exe+32B758] // ptr
  push 00
  push 000008C0 // _LD
  call eqgame.exe+B48A4 // get string by id from eqstr_xx.txt

  // append to name
  push eax
  lea eax,[maphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_ld_end

appendtoname_ld_end:

  jmp appendtoname_end

appendtoname_end:

  mov ecx,[maphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  mov eax,[maphack_spawn_y]
  add eax,[maphack_origin_y]
  sub eax,[maphack_player_y]
  mov [maphack_text_y],eax

  mov eax,[maphack_spawn_x]
  add eax,[maphack_origin_x]
  sub eax,[maphack_player_x]
  mov [maphack_text_x],eax

  cmp ecx,[eqgame.exe+32B7A0] // player spawn
  je drawarrow_begin
  jmp drawarrow_end

drawarrow_begin:

  fld [maphack_player_heading]
  fsub [maphack_128f] // angle fix, have to rotate by -90 degrees (512 / 128 = 4)
  fstp dword ptr [esp]
  call dword ptr [eqgame.exe+39D7C8] // EQGfx_Dx8.t3dFloatSine (math sin)
  fmul [maphack_arrow_distance]
  fstp dword ptr [maphack_arrow_add_y]

  fld [maphack_player_heading]
  fsub [maphack_128f] // angle fix, have to rotate by -90 degrees (512 / 128 = 4)
  fstp dword ptr [esp]
  call dword ptr [eqgame.exe+39D7BC] // EQGfx_Dx8.t3dFloatCosine (math cos)
  fmul [maphack_arrow_distance]
  fstp dword ptr [maphack_arrow_add_x]

  ////

  fld [maphack_player_yf]
  fadd [maphack_arrow_add_y]
  fistp [maphack_arrow_y]

  fld [maphack_player_xf]
  fsub [maphack_arrow_add_x]
  fistp [maphack_arrow_x]

  ////

  mov eax,[maphack_arrow_y]
  add eax,[maphack_origin_y]
  sub eax,[maphack_player_y]
  mov [maphack_arrow_y],eax

  mov eax,[maphack_arrow_x]
  add eax,[maphack_origin_x]
  sub eax,[maphack_player_x]
  mov [maphack_arrow_x],eax

  ////

  push 0E // color: green
  push [maphack_arrow_y] // y
  push [maphack_arrow_x] // x
  lea eax,[maphack_arrow_text]
  push eax
  //call eqgame.exe+314D4 // EQ_LoadingS::WriteTextHD
  call eqgame.exe+11D68 // CDisplay::WriteTextHD2

  mov ecx,[maphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  jmp drawarrow_end

drawarrow_end:

  //

  jmp textcolor_begin

textcolor_begin:

  cmp ecx,[eqgame.exe+32B7A0] // player spawn
  je textcolor_you

  cmp ecx,[eqgame.exe+329E38] // group member #1
  je textcolor_group

  cmp ecx,[eqgame.exe+329E3C] // group member #2
  je textcolor_group

  cmp ecx,[eqgame.exe+329E40] // group member #3
  je textcolor_group

  cmp ecx,[eqgame.exe+329E44] // group member #4
  je textcolor_group

  cmp ecx,[eqgame.exe+329E48] // group member #5
  je textcolor_group

  cmp byte ptr [ecx+0000017E],01 // gamemaster
  je textcolor_gamemaster

  cmp byte ptr [ecx+0000016C],00 // type is player
  je textcolor_player

  cmp byte ptr [ecx+0000016C],02 // type is npc corpse
  je textcolor_corpse

  cmp byte ptr [ecx+0000016C],03 // type is player corpse
  je textcolor_corpse

  jmp textcolor_npc

textcolor_you:

  push 0E // green
  jmp textcolor_end

textcolor_player:

  push 0D // red
  jmp textcolor_end

textcolor_group:

  push 0E // green
  jmp textcolor_end

textcolor_npc:

  cmp dword ptr [ecx+00000080],00 // movement speed
  jne textcolor_npc_moving

  //push 01 // dark grey
  //push 04 // dark blue
  //push 10 // medium blue
  push 12 // light blue
  jmp textcolor_end

textcolor_npc_moving:

  //push 0C // light grey
  push 12 // light blue
  jmp textcolor_end

textcolor_corpse:

  push 0F // yellow
  jmp textcolor_end

textcolor_gamemaster:

  push 05 // pink
  jmp textcolor_end

textcolor_end:

  push [maphack_text_y] // y
  push [maphack_text_x] // x

  jmp name_begin

name_begin:

  cmp ecx,[eqgame.exe+32B7A0] // player spawn
  je name_player

  push maphack_spawn_name_display
  jmp name_end

name_player:
  push maphack_player_name
  jmp name_end

name_end:

  //call eqgame.exe+314D4 // EQ_LoadingS::WriteTextHD
  call eqgame.exe+11D68 // CDisplay::WriteTextHD2

  jmp targetline_begin

targetline_begin:

  cmp [maphack_draw_line_to_target],01
  jne targetline_end

  mov ecx,[eqgame.exe+32B7AC] // target spawn
  test ecx,ecx
  jz targetline_end

  fld [ecx+00000068] // y
  fmul [maphack_zoom]
  fchs // reverse sign
  fstp [maphack_target_yf]
  fld [ecx+0000006C] // x
  fmul [maphack_zoom]
  fchs
  fstp [maphack_target_xf]

  fld [ecx+00000068] // y
  fmul [maphack_zoom]
  fchs // reverse sign
  fistp [maphack_target_y] // store float as int
  fld [ecx+0000006C] // x
  fmul [maphack_zoom]
  fchs
  fistp [maphack_target_x]
  fld [ecx+00000070] // z
  fistp [maphack_target_z]

  // line from x
  fild [maphack_origin_x]
  fstp [maphack_target_line]

  // line from y
  fild [maphack_origin_y]
  fstp [maphack_target_line+04]

  // line from z
  fld1
  fstp [maphack_target_line+08]

  // line to x
  mov eax,[maphack_target_x]
  add eax,[maphack_origin_x]
  sub eax,[maphack_player_x]
  mov [maphack_target_temp],eax
  fild [maphack_target_temp]
  fstp [maphack_target_line+0C]

  // line to y
  mov eax,[maphack_target_y]
  add eax,[maphack_origin_y]
  sub eax,[maphack_player_y]
  mov [maphack_target_temp],eax
  fild [maphack_target_temp]
  fstp [maphack_target_line+10]

  // line to z
  fld1
  fstp [maphack_target_line+14]

  mov eax,FF008000 // line color ARGB
  push eax
  lea eax,[maphack_target_line] // line structure
  push eax
  call dword ptr [eqgame.exe+39D74C] // EQGfx_Dx8.t3dDeferLine

  jmp targetline_end

targetline_end:

  //

  jmp spawn_next

spawn_next:

  mov ecx,[maphack_spawn_holder]
  mov ecx,[ecx+0000013C] // get next spawn
  test ecx,ecx // check if next spawn is null
  jz code2

  mov [maphack_spawn_holder],ecx

  jmp spawn_loop

code2:

  // restore the font size / style
  mov ecx,[eqgame.exe+39CD38] // ptr to font info from CDisplay::WriteTextHD2
  mov ecx,[ecx+000000D4]
  mov ecx,[ecx+00000008]
  mov [ecx+00000004],02 // offset to font size / style

  leave
  ret

address:
  jmp code
return:

[DISABLE]

address:
  db bytes
  // call eqgame.exe+19B2D9

