esphack_spawn_name_string:
db 2B 20 25 73 00
// + %s

esphack_spawn_level_string:
db 20 4C 25 64 00
// _L%d

esphack_spawn_distance_string:
db 20 28 25 64 29 00
// _(%d)
// {Distance}

esphack_draw_distance:
dd (float)400.0

esphack_show_distance:
db 01

esphack_font_style:
db 03
// default value is 02, valid values are 01-09
// value of 03 is less blurry and easy to read
// value of 06 or 08 is huge bold text
// value is 09 is the smallest text

esphack_draw_line_to_target:
db 01

esphack:

code:
  // original code
  call eqgame.exe+19B2D9
  add ebp,68

  // start of our code

  mov byte ptr [eqgame.exe+2C903C],01 // enable draw net status

  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  cmp dword ptr [ecx+000005B4],05 // check if game state equals in game
  jne code2

  mov ecx,[eqgame.exe+32B7A0] // player spawn
  test ecx,ecx // check if spawn is NULL
  jz code2

  cmp byte ptr [ecx+000000E6],01 // is linkdead
  je code2

  mov ecx,[eqgame.exe+32B768] // spawn begin
  test ecx,ecx // check if spawn is NULL
  jz code2

  mov [esphack_spawn_holder],ecx

  // set the font size / style
  mov eax,[esphack_font_style]
  test eax,eax
  jz code2

  mov ecx,[eqgame.exe+39CD38] // ptr to font info from CDisplay::WriteTextHD2
  test ecx,ecx
  jz code2
  mov ecx,[ecx+000000D4]
  test ecx,ecx
  jz code2
  mov ecx,[ecx+00000008]
  test ecx,ecx
  jz code2
  mov [ecx+00000004],eax // offset to font size / style

  jmp spawn_loop

spawn_loop:

  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  cmp dword ptr [ecx+000005B4],05 // check if game state equals in game
  jne code2

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  cmp ecx,[eqgame.exe+32B7A0] // player spawn
  je spawn_next // ignore player

  fldz
  fstp dword ptr [esp]
  mov ecx,[eqgame.exe+32B7CC] // CDisplay
  push 0
  push [eqgame.exe+32B7A0] // player spawn
  push [esphack_spawn_holder]
  call eqgame.exe+DD20 // CDisplay::PlayerDistance
  fstp dword ptr [esphack_spawn_distance]

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  // target is visible from any distance
  cmp ecx,[eqgame.exe+32B7AC] // target spawn
  je drawdistance_end

  // group is visible from any distance
  cmp ecx,[eqgame.exe+329E38] // group member #1
  je drawdistance_end

  cmp ecx,[eqgame.exe+329E3C] // group member #2
  je drawdistance_end

  cmp ecx,[eqgame.exe+329E40] // group member #3
  je drawdistance_end

  cmp ecx,[eqgame.exe+329E44] // group member #4
  je drawdistance_end

  cmp ecx,[eqgame.exe+329E48] // group member #5
  je drawdistance_end

  // players are always visible
  cmp byte ptr [ecx+0000016C],00 // type is player
  je drawdistance_end

  // gamemaster is always visible
  cmp byte ptr [ecx+0000017E],01 // gamemaster
  je drawdistance_end

  jmp drawdistance_begin

drawdistance_begin:

  // check if spawn is within draw distance
  fld dword ptr [esphack_spawn_distance]
  fcomp dword ptr [esphack_draw_distance]
  fnstsw ax
  sahf
  ja spawn_next

  jmp drawdistance_end

drawdistance_end:

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  fld [ecx+00000068] // y
  fstp [esphack_spawn_point]
  fld [ecx+0000006C] // x
  fstp [esphack_spawn_point+04]
  fld [ecx+00000070] // z
  fstp [esphack_spawn_point+08]

  lea eax,[esphack_text_y] // screen y gets stored here as float
  push eax
  lea eax,[esphack_text_x] // screen x gets stored here as float
  push eax
  lea eax,[esphack_spawn_point] // vec3_t structure of world y x z
  push eax
  mov eax,[eqgame.exe+2152BC] // WorldSpaceToScreenSpace camera view angles pointer
  push eax
  call EQGfx_Dx8.t3dWorldSpaceToScreenSpace // EQGfx_Dx8.dll+4B4C0
  cmp eax,FFFF3D3E // point not on screen, not viewable, skip it
  je spawn_next

  fld [esphack_text_y]
  call eqgame.exe+19B11C // _ftol2
  mov [esphack_text_y],eax

  fld [esphack_text_x]
  call eqgame.exe+19B11C // _ftol2
  mov [esphack_text_x],eax

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  lea eax,[ecx+00000001] // offset to name
  mov ecx,[eqgame.exe+388DE0] // CEverQuest
  push eax
  call eqgame.exe+9087C // trim name to have no zeros or underscores
  push eax
  lea eax,[esphack_spawn_name_display] // string that holds the result of sprintf
  push esphack_spawn_name_string // + %s
  push eax
  call eqgame.exe+19B58B // _sprintf

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  jmp appendtoname_begin

appendtoname_begin:

  cmp byte ptr [ecx+0000016C],00 // type is player
  jne appendtoname_level_begin

  cmp byte ptr [ecx+00000194],01 // is afk
  je appendtoname_afk_begin
  jmp appendtoname_afk_end

appendtoname_afk_begin:
  mov ecx,[eqgame.exe+32B758] // ptr
  push 00
  push 00003017 // _AFK
  call eqgame.exe+B48A4 // get string by id from eqstr_xx.txt

  // append to name
  push eax
  lea eax,[esphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_afk_end

appendtoname_afk_end:
  mov ecx,[esphack_spawn_holder]
  cmp byte ptr [ecx+000000E6],01 // is linkdead
  je appendtoname_ld_begin
  jmp appendtoname_ld_end

appendtoname_ld_begin:
  mov ecx,[eqgame.exe+32B758] // ptr
  push 00
  push 000008C0 // _LD
  call eqgame.exe+B48A4 // get string by id from eqstr_xx.txt

  // append to name
  push eax
  lea eax,[esphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_ld_end

appendtoname_ld_end:

  jmp appendtoname_level_begin

appendtoname_level_begin:

  mov ecx,[esphack_spawn_holder]
  movzx eax, byte ptr [ecx+00000174]
  push eax
  lea eax,[esphack_spawn_level_display] // string that holds the result of sprintf
  push esphack_spawn_level_string // _L%d
  push eax
  call eqgame.exe+19B58B // _sprintf

  // append to name
  lea eax,[esphack_spawn_level_display]
  push eax
  lea eax,[esphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_level_end

appendtoname_level_end:

  jmp appendtoname_distance_begin

appendtoname_distance_begin:

  cmp [esphack_show_distance],01
  jne appendtoname_end

  fld dword ptr [esphack_spawn_distance]
  call eqgame.exe+19B11C // _ftol2
  push eax
  lea eax,[esphack_spawn_distance_display] // string that holds the result of sprintf
  push esphack_spawn_distance_string // _(%d)
  push eax
  call eqgame.exe+19B58B // _sprintf

  // append to name
  lea eax,[esphack_spawn_distance_display]
  push eax
  lea eax,[esphack_spawn_name_display]
  push eax
  call eqgame.exe+19B1C0 //_strcat

  jmp appendtoname_distance_end

appendtoname_distance_end:

  jmp appendtoname_end

appendtoname_end:

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  jmp textcolor_begin

textcolor_begin:

  cmp ecx,[eqgame.exe+32B7A0] // player spawn
  je textcolor_you

  cmp ecx,[eqgame.exe+32B7AC] // target spawn
  je textcolor_target

  cmp ecx,[eqgame.exe+329E38] // group member #1
  je textcolor_group

  cmp ecx,[eqgame.exe+329E3C] // group member #2
  je textcolor_group

  cmp ecx,[eqgame.exe+329E40] // group member #3
  je textcolor_group

  cmp ecx,[eqgame.exe+329E44] // group member #4
  je textcolor_group

  cmp ecx,[eqgame.exe+329E48] // group member #5
  je textcolor_group

  cmp byte ptr [ecx+0000017E],01 // gamemaster
  je textcolor_gamemaster

  cmp byte ptr [ecx+0000016C],00 // type is player
  je textcolor_player

  cmp byte ptr [ecx+0000016C],02 // type is npc corpse
  je textcolor_corpse

  cmp byte ptr [ecx+0000016C],03 // type is player corpse
  je textcolor_corpse

  jmp textcolor_npc

textcolor_you:

  push 0E // green
  jmp textcolor_end

textcolor_target:

  push 05 // pink
  jmp textcolor_end

textcolor_player:

  push 0D // red
  jmp textcolor_end

textcolor_group:

  push 0E // green
  jmp textcolor_end

textcolor_npc:

  cmp dword ptr [ecx+00000080],00 // movement speed
  jne textcolor_npc_moving

  //push 01 // dark grey
  //push 04 // dark blue
  //push 10 // medium blue
  push 12 // light blue
  jmp textcolor_end

textcolor_npc_moving:

  //push 0C // light grey
  push 12 // light blue
  jmp textcolor_end

textcolor_corpse:

  push 0F // yellow
  jmp textcolor_end

textcolor_gamemaster:

  push 05 // pink
  jmp textcolor_end

textcolor_end:

  push [esphack_text_y] // y
  push [esphack_text_x] // x

  push esphack_spawn_name_display // text

  //call eqgame.exe+314D4 // EQ_LoadingS::WriteTextHD
  call eqgame.exe+11D68 // CDisplay::WriteTextHD2

  jmp targetline_begin

targetline_begin:

  cmp [esphack_draw_line_to_target],01
  jne targetline_end

  mov ecx,[esphack_spawn_holder]
  test ecx,ecx // check if spawn is NULL
  jz code2

  // check if the current spawn is your target
  cmp ecx,[eqgame.exe+32B7AC] // target spawn
  jne targetline_end

  // line from x
  mov ecx,[eqgame.exe+2152C0] // camera
  mov eax,[ecx+00000048] // resolution width
  sar eax,01 // divide by 2
  mov [esphack_target_temp],eax
  fild [esphack_target_temp]
  fstp [esphack_target_line]

  // line from y
  mov ecx,[eqgame.exe+2152C0] // camera
  mov eax,[ecx+0000004C] // resolution height
  mov [esphack_target_temp],eax
  fild [esphack_target_temp]
  fstp [esphack_target_line+04]

  // line from z
  fld1
  fstp [esphack_target_line+08]

  // line to x
  fild [esphack_text_x]
  fstp [esphack_target_line+0C]

  // line to y
  fild [esphack_text_y]
  fstp [esphack_target_line+10]

  // line to z
  fld1
  fstp [esphack_target_line+14]

  mov eax,FFFF00FF // line color ARGB
  push eax
  lea eax,[esphack_target_line] // line structure
  push eax
  call dword ptr [eqgame.exe+39D74C] // EQGfx_Dx8.t3dDeferLine

  jmp targetline_end

targetline_end:

  jmp spawn_next

spawn_next:

  mov ecx,[esphack_spawn_holder]
  mov ecx,[ecx+0000013C] // get next spawn
  test ecx,ecx // check if next spawn is null
  jz code2

  mov [esphack_spawn_holder],ecx

  jmp spawn_loop

code2:

  // restore the font size / style
  mov ecx,[eqgame.exe+39CD38] // ptr to font info from CDisplay::WriteTextHD2
  mov ecx,[ecx+000000D4]
  mov ecx,[ecx+00000008]
  mov [ecx+00000004],02 // offset to font size / style

  leave
  ret

address:
  jmp code
return:

